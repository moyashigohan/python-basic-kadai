        "def calculate_total_price(price, tax_rate):\n",
        "\n",
        "    total_price = price * (1 + tax_rate / 100)\n",
        "    return total_price\n",
        "\n",
        "\n",
        "price = 110\n",
        "tax_rate = 10\n",
        "result = calculate_total_price(price, tax_rate)\n",
        "print(f\"消費税込みの合計金額: {result:.2f}円\")\n"
